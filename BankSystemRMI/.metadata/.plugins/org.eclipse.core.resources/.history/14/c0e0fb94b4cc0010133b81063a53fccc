package util;
import java.sql.*;

public class Conn {


	  private static final String URL = "jdbc:mysql://localhost:3306/bank_management_system?useSSL=false&serverTimezone=UTC";
	  private static final String USER = "root";
	  private static final String PASS = "MASTERIISE@";
	  public static Connection getConnection () 
		{	
			try
			{

				return DriverManager.getConnection(URL, username, password);
			}
			catch(Exception e)
			{
	            e.printStackTrace();
	            return null;

			}
		}	
}
