package server;

import java.Naming;
import java.registry.LocateRegistry;

public class ServerMain {
    public static void main(String[] args) {
        try {
            LocateRegistry.createRegistry(1099);
            BankServiceImpl impl = new BankServiceImpl();
            Naming.rebind("rmi://localhost:1099/bankService", impl);
            System.out.println("RMI Server started on localhost:1099");
        } catch(Exception e) {
            e.printStackTrace();
        }
    }
}
