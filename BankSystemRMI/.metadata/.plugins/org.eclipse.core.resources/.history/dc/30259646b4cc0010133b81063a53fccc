package util;
import java.sql.*;

public class Conn {

		Connection c;
		Statement s;
		public Conn () 
		{	
		    private static final String URL = "jdbc:mysql://localhost:3306/bank_management_system?useSSL=false&serverTimezone=UTC";
			String username = "root";
			String password = "MASTERIISE@";
			try
			{
				c = DriverManager.getConnection(URL, username, password);
			}
			catch(Exception e)
			{
	            e.printStackTrace();
	            return null;

			}
		}	
}
