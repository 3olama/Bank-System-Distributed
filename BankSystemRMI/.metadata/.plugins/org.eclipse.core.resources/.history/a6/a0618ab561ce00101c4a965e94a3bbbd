package rmi;

public class Agence {
    private int agenceId;
    private int bankId;
    private String nameAgence;
    private String address;
    private String phone;

    public Agence(int agenceId, int bankId, String nameAgence, String address, String phone) {
        this.agenceId = agenceId;
        this.bankId = bankId;
        this.nameAgence = nameAgence;
        this.address = address;
        this.phone = phone;
    }

    public int getAgenceId() { return agenceId; }
    public int getBankId() { return bankId; }
    public String getNameAgence() { return nameAgence; }
    public String getAddress() { return address; }
    public String getPhone() { return phone; }

    @Override
    public String toString() {
        return nameAgence + " - " + address; // Ce qui s'affiche dans le JComboBox
    }
}
