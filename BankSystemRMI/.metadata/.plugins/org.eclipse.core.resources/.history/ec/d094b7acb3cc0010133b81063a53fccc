package util;
import java.sql.*;

public class Conn {

		Connection c;
		Statement s;
		public Conn () 
		{	
			String username = "root";
			String password = "MASTERIISE@";
			try
			{
				c = DriverManager.getConnection("jdbc:mysql:///bank_management_system?useSSL=false&serverTimezone=UTC", username, password);
			}
			catch(Exception e)
			{
	            e.printStackTrace();
	            return null;

			}
		}	
}
